---
import Layout from '@/layouts/Public.layout.astro'
import {
  Biografia,
  CenizasAnimate,
  Discos,
  Merch,
  Noticias,
} from '@/contents/Home'
import Navbar from '@/components/navbar/Navbar.component.astro'
---

<Layout title="The Waris | Home ">
  <link
    rel="stylesheet"
    type="text/css"
    charset="UTF-8"
    href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick.min.css"
  />
  <link
    rel="stylesheet"
    type="text/css"
    href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick-theme.min.css"
  />
  <CenizasAnimate client:load />
  <Navbar />
  <Biografia client:load />
  <Merch client:load />
  <Noticias client:load />
  <Discos client:load />
</Layout>
